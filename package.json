{
  "name": "yektacare",
  "version": "0.1.0",
  "description": "YektaCare - Mental Health Care Platform",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "dev:backend": "npm run dev -w @yektacare/backend",
    "dev:admin": "npm run dev -w @yektacare/admin-panel",
    "dev:mobile": "npm run dev -w @yektacare/mobile-app",
    "build": "npm run build --workspaces --if-present",
    "build:backend": "npm run build -w @yektacare/backend",
    "build:admin": "npm run build -w @yektacare/admin-panel",
    "build:mobile": "npm run build -w @yektacare/mobile-app",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist",
    "install:all": "npm install"
  },
  "keywords": [
    "mental-health",
    "healthcare",
    "monorepo",
    "vue",
    "ionic",
    "elysia"
  ],
  "author": "YektaCare Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
